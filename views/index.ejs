<!DOCTYPE html>
<html lang="en">

<head>
    <!--Useful meta tags-->
    <meta name="robots" content="index, follow, noodp" />
    <meta name="googlebot" content="index, follow" />
    <meta name="google" content="notranslate" />
    <meta charset="UTF-8" />
    <meta name="description" content="<%= owner.username %> is a Software Developer and is only 16!
Click to find out more!">
<meta property="og:title" content="<%= owner.username %>">
    <meta property="og:description" content="<%= owner.username %> is a Software Developer and is only 16!
Click to find out more!">
    <meta property="og:image" content="https://cdn.discordapp.com/avatars/<%= owner.id %>/<%= owner.avatar %>.gif?size=1024">
    
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <!--Preloader JS-->
    <script type="text/javascript" src="./assets/js/preloader.js"></script>
    <!--Preloader Style-->
    <link rel="stylesheet" href="./assets/css/preload.css">
    
    <!--Bootstrap Stylesheet-->
    <link rel="stylesheet" href="./assets/css/bootstrap.min.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Magnific Popup Stylesheet-->
    <link rel="stylesheet" href="./assets/css/magnific-popup.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--OWL Carousel Stylesheet-->
    <link rel="stylesheet" href="./assets/css/owl.carousel.min.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Font Awesome Stylesheet-->
    <link rel="stylesheet" href="./assets/css/all.min.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Mobirise-->
    <link rel="stylesheet" href="./assets/css/mobirise/style.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--AOC-->
    <link rel="stylesheet" href="./assets/css/aos.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Main Stylesheet-->
    <link rel="stylesheet" href="./assets/css/main.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Responsive Stylesheet-->
    <link rel="stylesheet" href="./assets/css/responsive.css" as="style"
        onload="this.rel='stylesheet';this.removeAttribute('onload');">
    <!--Theme Stylesheet-->
    <link rel="stylesheet" href="./assets/css/dark.theme.css" id="optional-theme" />
    <!--Theme Color-->
    <link rel="stylesheet" href="./assets/css/default.css" id="optional-color" />
    <!--MAIN TITLE-->
    <title>Akeno Dev :: Home</title>


    <!-- Custom Cursor CSS -->

    <style>
        /* Make the cursor ./assets/cursors/cursor.cur */
        /* * {
            cursor: url(./assets/cursors/cursor.cur), auto;
        } */
    </style>


</head>

<body data-spy="scroll" data-target=".scrollspy" data-offset="85">
    <!--PRELOADER START-->
    <div id="preloader">
        <div class="text-center w-100">
            <div class="loader"></div>
        </div>
    </div>
    <!--PRELOADER END-->
    <!--HEADER START-->
    <header class="navigation horizontal_header transparentOnScroll navbar navbar-expand-lg fixed-top">
        <div class="header-ins">
            <div class="container">
                <!--LOGO AREA-->
                <div class="site-logo">
                    <a href="#" class="hoveroff">
                       Akeno<span>Dev</span>
                    </a>
                </div>
                <button class="button-toggler navbar-toggler collapsed" type="button" data-target="#navigation"
                    data-toggle="collapse" aria-controls="navigation" aria-expanded="false">
                    <span></span>
                    <span></span>
                    <span></span>
                </button>
                <!--Main Navigation START-->
                <ul class="justify-content-end scrollspy collapse navbar-collapse" id="navigation">
                    <li>
                        <a href="#home" class="nav-link">Home</a>
                    </li>
                    <li>
                        <a href="#about" class="nav-link">About</a>
                    </li>
                    <li>
                        <a href="#service" class="nav-link">Services</a>
                    </li>
                    <li>
                        <a href="#portfolio" class="nav-link">Portfolio</a>
                    </li>
                    <li>
                        <a href="#resume" class="nav-link">Resume</a>
                    </li>

                    <li>
                        <a href="#skills" class="nav-link">Skills</a>
                    </li>

                    <li>
                        <button class="btn btn-primary" data-toggle="modal" data-target="#discordModal">Join
                            Discord</button>
                    </li>
                </ul>
                <!--Main Navigation END-->

            </div>
        </div>

    </header>
    <!--HEADER END-->

    <main class="content center-main">
        <!--INTRODUCTION SECTION START-->
        <section class="home d-flex" id="home">
            <!--Parallax Background-->
            <div class="background-parallax" id="parallaxbackground">
                <div class="element1 element red" data-depth="0.8"></div>
                <div class="element2 element green" data-depth="0.5"></div>
                <div class="element3 element blue" data-depth="0.2"></div>
                <div class="element4 element red" data-depth="0.4"></div>
                <div class="element5 element orange" data-depth="0.3"></div>
                <div class="element6 element blue" data-depth="0.2"></div>
            </div>
            <!--End of Parallax Background-->
            <div class="container introduction text-center">
                <p class="welcome_text mb-4" data-aos="fade-up" data-aos-duration="1200" data-aos-delay="150">I'm a
                    <span class="welcome-text-type"
                        data-options="<%= aboutMe %> "></span>
                </p>
                <div id="parallax-scene" class="mb-4" data-aos="fade-up" data-aos-duration="1200" data-aos-delay="300">
                    <div class="heading-zone">
                        <h1>
                            <%= owner.username %>
                        </h1>
                    </div>
                </div>
                <div data-aos="fade-up" data-aos-duration="1200" data-aos-delay="450">
                    <a href="#about" class="btn  btn-primary btn-small"><i class="fa-solid fa-file"></i> Learn More</a>
                    <button class="btn  btn-primary btn-small" onclick="window.location.href='/reportbug'" > <i class="fa-solid fa-bug"></i> Report a Bug</button>
                    <button class="btn btn-secondary btn-small" data-toggle="modal" data-target="#exampleModal"><i class="fa-solid fa-hashtag"></i> Social Media</button>
                    
                </div>
            </div>
        </section>
        <!--INTRODUCTION SECTION END-->


        <!--ABOUT SECTION START-->
        <section class="about" id="about">
            <div class="container d-flex justify-content-between flex-column h-100">
                <div class="section-title flex-grow-1">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="text-center">
                                <h2 data-aos="heading-show" class="rect_heading">Who Am I</h2>
                                <p class="m-0">Get to know me</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="section-content flex-grow-1">
                    <div class="row justify-content-center align-items-center">
                        <div class="col-md-6">
                            <div class="avatar-area m-auto"
                            style="width: 450; height: 450;  border-radius: 50%;">
                            
                                <% if (owner.avatar.includes("a_")) { %>
                                    <img src="https://cdn.discordapp.com/avatars/<%= owner.id %>/<%= owner.avatar %>.gif?size=1024"
                                        width="400" height="550" alt="" class="img-fluid mx-auto">
                                    <% } %>
                                        <% if (!owner.avatar.includes("a_")) { %>
                                            <img src="https://cdn.discordapp.com/avatars/<%= owner.id %>/<%= owner.avatar %>.png?size=1024"
                                                width="400" height="550" alt="" class="img-fluid mx-auto">
                                            <% } %>
                            </div>
                        </div>
                        <div class="col-md-6 mt-5 mt-md-0">
                            <h3 class="m-0">I'm <%= owner.username %> ðŸ‘‹</h3>
                            <p class="my-4">
                                <%= bio %>
                            </p>
                            <div class="row">
                                <div class="col-lg-6">
                                    
                                    <div class="about-info mb-2">
                                        <i class="fa-solid fa-id-card"></i>
                                        <span class="desc">Name:</span>
                                        <span>Hayden (<%= owner.username %>) </span>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-brands fa-pagelines"></i>
                                        <span class="desc">Age:</span>
                                        <span>16 Years Old</span>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-solid fa-location-dot"></i>
                                        <span class="desc">Location:</span>
                                        <span>
                                            <%= location %>
                                        </span>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-solid fa-user"></i>
                                        <span class="desc">Languages:</span>
                                        <span>
                                            <%= languages %>
                                        </span>
                                    </div>
                                </div>
                               
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-solid fa-briefcase"></i>
                                        <span class="desc">Looking for Jobs:</span>
                                        <% if (isHireable===true) { %>
                                            <span><a href="https://github.com/akenolol" class="__cf_email__"
                                                    data-cfemail="93f8f6e5fafdfefafffff6e1d3f8fff1bdf0fcfe">Yes</a></span>
                                            <% } %>
                                                <% if (isHireable===false) { %>
                                                    <span><a href="https://github.com/akenolol" class="__cf_email__"
                                                            data-cfemail="93f8f6e5fafdfefafffff6e1d3f8fff1bdf0fcfe">No</a></span>
                                                    <% } %>

                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-solid fa-tag"></i>
                                        <span class="desc">Company:</span>
                                        <span><a href="https://github.com/<%= companyReal %>">
                                                <%= company %>
                                            </a></span>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-brands fa-discord"></i>
                                        <span class="desc"> Discord Server: </span>
                                        <span><a href="<%= supportServer %> ">
                                                discord.gg/
                                            </a></span>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="about-info mb-2">
                                        <i class="fa-brands fa-twitter"></i>
                                        <span class="desc"> Twitter:</span>
                                        <span><a href="<%= socials.twitter %>">@akeno_dev</a></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section>
        <!--ABOUT SECTION END-->

        <!--FACTS SECTION START-->
        <div class="section facts backgound-section"
            style="background-image: url(https://cdn.discordapp.com/attachments/981632133191843852/1025131890832834591/unknown.png)"
            data-aos-id="facts">

            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-sm-6 mt-3 mt-lg-0">
                        <div class="fact-single text-center">
                            <div class="fact-icon">
                                <span class="mbri-timer"></span>
                            </div>
                            <div class="fact-info">
                                <span class="tmcounter" data-from="0" data-to="<%= followers %> " data-speed="1500">
                                    <%= followers %>
                                </span>
                            </div>
                            <div class="fact-meta">
                                <span>Followers</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-3 mt-lg-0">
                        <div class="fact-single text-center">
                            <div class="fact-icon">
                                <span class="mbri-smile-face"></span>
                            </div>
                            <div class="fact-info">
                                <span class="tmcounter" data-from="0" data-to="<%= repos %>" data-speed="1500">
                                    <%= repos %>
                                </span>
                            </div>
                            <div class="fact-meta">
                                <span>Repos</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-3 mt-lg-0">
                        <div class="fact-single text-center">
                            <div class="fact-icon">
                                <span class="mbri-magic-stick"></span>
                            </div>
                            <div class="fact-info">
                                <span class="tmcounter" data-from="0" data-to="50" data-speed="1500">50</span>
                            </div>
                            <div class="fact-meta">
                                <span>Projects Done</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-3 mt-lg-0">
                        <div class="fact-single text-center">
                            <div class="fact-icon">
                                <span class="mbri-save"></span>
                            </div>
                            <div class="fact-info">
                                <span class="tmcounter" data-from="0" data-to="60" data-speed="1500">60</span>
                            </div>
                            <div class="fact-meta">
                                <span>Discord / Telegram Bots Completed</span>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!--FACTS SECTION END-->

        <!--SERVICES SECTION START-->
        <section class="services" id="service">
            <div class="container d-flex flex-column justify-content-between h-100 ">

                <div class="section-title flex-grow-1">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="text-center">
                                <h2 data-aos="heading-show" class="rect_heading">What I Do</h2>
                                <p class="m-0">Here is what i do!</p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="section-content flex-grow-1">
                    <div class="row">
                        <div class="col-lg-4 col-md-6 mt-4 mb-2">
                            <div class="service-single text-center p-5">
                                <div class="service-icon">
                                    <i class="fa-solid fa-code-branch"></i>
                                </div>
                                <h3 class="my-3">Development</h3>
                                <p>I Create <strong>GAMES</strong>, <strong>APPS</strong>, <strong>WEBSITES</strong>,
                                    and <strong>BOTS</strong>.</p>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6 mt-4 mb-2">
                            <div class="service-single text-center p-5">
                                <div class="service-icon">
                                    <i class="fa-solid fa-shield"></i>
                                </div>
                                <h3 class="my-3">Responsive</h3>
                                <p>I Create Quick, Easy, and Amazing Projects.</p>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6 mt-4 mb-2">
                            <div class="service-single text-center p-5">
                                <div class="service-icon">
                                    <i class="fa-solid fa-robot"></i>
                                </div>
                                <h3 class="my-3">Bot Development</h3>
                                <p>In My Spare Time I also create <a href="https://discord.com">Discord</a> Bots!</p>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </section>

        <!--SERVICE SECTION START-->

        

        <!--PORTFOLIO SECTION START-->
        <section class="portfolio" id="portfolio">
            <div class="container d-flex flex-column justify-content-between h-100 ">

                <div class="section-title flex-grow-1">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="text-center">
                                <h2 data-aos="heading-show" class="rect_heading">My Projects</h2>
                                <p class="m-0">There are currently <%= repos %>, projects I have on my <a
                                            href="https://github.com/akenolol">Github!</a> </p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="section-content flex-grow-1">
                    <div class="row portfolio-items">

                        <% projects.forEach(element=> { %>
                            <div class="col-xl-4 col-lg-4 col-md-6 mb-2 mt-4 frontenddevelopment">
                                <div class="portfolio-item">
                                    <a href="<%= element.url %> " class="portfolio-single">
                                        <div class="item-box">
                                            <div class="item-image">
                                                <img src="<%= element.image %>" width="350" height="410"
                                                    class="d-block img-fluid mx-auto" alt="">
                                            </div>
                                            <div class="item-details d-flex align-items-center">
                                                <div class="item-details-ins text-center w-100">
                                                    <div class="mb-2">
                                                        <span class="mbri-photos"></span>
                                                    </div>
                                                    <h3>
                                                        <%= element.name %>
                                                    </h3>
                                                    <p>
                                                        <%= element.description %>
                                                    </p>
                                                    <p>
                                                        <%= element.language %>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </div>

                            
                            <% }) %>



                    </div>
                    <div class="row">
                        <div class="col-12 text-center mt-5">
                            <a href="https://github.com/akenolol" class="btn btn-outline-primary"><span></span><i class="fa-solid fa-link"></i> See
                                More</a>
                        </div>
                    </div>

                </div>
            </div>
        </section>
        <!--PORTFOLIO SECTION START-->




        <!--RESUME SECTION START-->
        <section class="resume" id="resume">

            <div class="container d-flex flex-column justify-content-between h-100 ">

                <div class="section-title flex-grow-1">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="text-center">
                                <h2 data-aos="heading-show" class="rect_heading">My Experience</h2>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="section-content flex-grow-1">
                    <div class="row mt-4">
                        <div class="col-lg-6">
                            <h3 class="mb-4"><i class="fa-solid fa-school"></i> Education</h3>
                            <ul class="list-inline resume-list">
                                <li>
                                    <h4 class="mb-2"><a href="https://www.pennfoster.edu/">Penn Foster</a></h4>
                                    <span class="time">2021 - Present</span>
                                    <p class="my-2">Currently enrolled here :)</p>
                                </li>
                                <li>
                                    <h4 class="mb-2"><a href="http://www.ware.k12.ga.us/">Ware County High School</a>
                                    </h4>
                                    <span class="time">2021 - 2021</span>
                                    <p class="my-2">I was enrolled by default, but unenrolled.</p>
                                </li>

                            </ul>
                        </div>

                        <div class="col-lg-6">
                            <h3 class="mb-4"><i class="fa-solid fa-tags"></i> Experience</h3>
                            <ul class="list-inline resume-list">
                                <li>
                                    <h4 class="mb-2">Bot Developer</h4>
                                    <span class="time">2019 - Present</span>
                                    <p class="my-2">
                                        I Spent <strong>MOST</strong> of my time working on
                                        <a href="https://discord.com">Discord</a>
                                        Bots.
                                    </p>
                                </li>
                                <li>
                                    <h4 class="mb-2">Software Developer</h4>
                                    <span class="time">2019 - Present</span>
                                    <p class="my-2">
                                        I have been working on Software Development for a while now, and I have been
                                        learning a lot!
                                    </p>
                                </li>
                            </ul>
                        </div>


                    </div>
                </div>

            </div>

        </section>
        <!--RESUME SECTION START-->

        

        <% if (currentIssues.length != 0) { %>
            <section class="issues" id="issues">

                <div class="container d-flex flex-column justify-content-between h-100 ">
    
                    <div class="section-content flex-grow-1">
                        <div class="row mt-4">
                            <div class="col-lg-6">
                                <h3 class="mb-4"><i class="fa-solid fa-bug"></i> Current Issues</h3>
                                <ul class="list-inline resume-list">
                                    <% currentIssues.forEach(element => { %>
                                        <li>
                                            <h4 class="mb-2"><a href="https://github.com/akenolol/akenodev.xyz/issues/<%= element.number %> "><%= element.title %> </a>
                                            </h4>
                                            <span class="time">Issue Opened By: <%= element.user.login %> </span>
                                            <p class="my-2">State: <%= element.state %>  </p>
                                        </li>
                                    <% }) %>
                                    
    
                                </ul>
                            </div>
    
    
                        </div>
                    </div>
    
                </div>
    
            </section>
        <% } %>

        <% if (currentPullRequests.length != 0) { %>
            <section class="pull" id="pull">

                <div class="container d-flex flex-column justify-content-between h-100 ">
    
                    <div class="section-content flex-grow-1">
                        <div class="row mt-4">
                            <div class="col-lg-6">
                                <h3 class="mb-4"><i class="fa-solid fa-code-pull-request"></i> Pull Requests</h3>
                                <ul class="list-inline resume-list">
                                    <% currentPullRequests.forEach(element => { %>
                                        <li>
                                            <h4 class="mb-2"><a href="<%= element.url %> "><%= element.title %> </a>
                                            </h4>
                                            <span class="time">Issue Opened By: <%= element.user.login %> </span>
                                            <p class="my-2">State: <%= element.state %>  </p>
                                        </li>
                                    <% }) %>
                                    
    
                                </ul>
                            </div>
    
    
                        </div>
                    </div>
    
                </div>
    
            </section>
        <% } %>


        
        <!--RESUME SECTION START-->

        <section class="skills" id="skills">
            <div class="container d-flex flex-column justify-content-between h-100 ">

                <div class="section-title flex-grow-1">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="text-center">
                                <h2 data-aos="heading-show" class="rect_heading">My Skills</h2>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="section-content flex-grow-1">
                    <div class="row mt-4">
                        <div class="col-lg-6">
                            <h3 class="mb-4"><i class="fa-solid fa-file-code"></i></i> Programming Languages</h3>
                            <ul class="list-inline resume-list">
                                <div class="row mt-5">
                                    <% codingLanguages.forEach(element => { %>
                                        <div class="col-lg-6">
                                            <div class="skill d-flex flex-row pr-4 pr-sm-3 px-3 mb-4">
                                                <div class="skill-progress flex-grow-1">
                                                    <div class="progresss-info d-flex flex-row justify-content-between mt-2 mb-1">
                                                        <span><%= element.name %> </span>
                                                        <span class="text-right progress-percent"><%= element.options.percentile %>%</span>
                                                    </div>
                                                    <div class="progress">
                                                        <div class="progress-bar" role="progressbar" data-percent="<%= element.options.percentile %>"
                                                            aria-valuenow="<%= element.options.percentile %>" aria-valuemin="0" aria-valuemax="100"></div>
                                                    </div>
                                                </div>
                                            </div>
                
                                        </div>
                                    <% }) %>
                                    
                                   
            
                                    
                                </div>
                            </ul>
                            
                            
                        </div>
                        <div class="col-lg-6">
                            <h3 class="mb-4"><i class="fa-solid fa-box-archive"></i> Programming Frameworks</h3>
                            <ul class="list-inline resume-list">
                                <div class="row mt-5">
                                    <% frameworks.forEach(element => { %>
                                        <div class="col-lg-6">
                                            <div class="skill d-flex flex-row pr-4 pr-sm-3 px-3 mb-4">
                                                <div class="skill-progress flex-grow-1">
                                                    <div class="progresss-info d-flex flex-row justify-content-between mt-2 mb-1">
                                                        <span><%= element.name %> </span>
